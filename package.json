{
  "name": "primer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "npm run code-analysis && npm run test:nowatch && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:nowatch": "set CI=true && react-scripts test --env=jsdom",
    "code-analysis": "standard",
    "code-analysis:fix": "standard --fix",
    "eject": "react-scripts eject",
    "electron:dev": "nf start -p 3000",
    "electron:start": "node electron-wait",
    "electron": "electron ."
  },
  "pre-commit": [
    "test:nowatch",
    "code-analysis"
  ],
  "standard": {
    "env": [
      "jest"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "electron": "^6.0.8",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "foreman": "^3.0.1",
    "pre-commit": "^1.2.2",
    "react-bootstrap": "^1.0.0-beta.9",
    "standard": "^13.1.0"
  }
}
